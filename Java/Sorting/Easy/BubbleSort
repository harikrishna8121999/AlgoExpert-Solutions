import java.util.*;

class Program {
  public static int[] bubbleSort(int[] array) {
    boolean isSorted = false ;
    int temp =0;
    while(!isSorted){
      isSorted = true;
      for(int i =0;i< array.length-1;i++){
        if(array[i]>array[i+1]){
          temp = array[i];
          array[i] = array[i+1];
          array[i+1] = temp;
          isSorted = false;
        }
      }
    }
    // Write your code here.
    return array;
  }
}
